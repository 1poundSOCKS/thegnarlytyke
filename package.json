{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest --verbose",
    "build-index": "browserify source/index.page.js -o public/scripts/index.page.js",
    "build-crag-index-edit": "browserify source/crag-index-edit.page.js -o public/scripts/crag-index-edit.page.js",
    "build-crag-view": "browserify source/crag-view.page.js -o public/scripts/crag-view.page.js",
    "build-crag-edit": "browserify source/crag-edit.page.js -o public/scripts/crag-edit.page.js",
    "build-logon": "browserify source/logon.page.js -o public/scripts/logon.page.js",
    "build": "npm run build-index && npm run build-crag-index-edit && npm run build-crag-view && npm run build-crag-edit && npm run build-logon",
    "watch": "watchify source/bundle.js -o public/bundle.js -v",
    "web-test-to-live": "aws s3 sync s3://test.thegnarlytyke.com s3://thegnarlytyke.com --exclude 'config.json' --delete",
    "data-dev-to-test": "aws s3 sync s3://dev.data.thegnarlytyke.com s3://test.data.thegnarlytyke.com --delete",
    "data-test-to-live-step1": "aws s3 sync s3://data.thegnarlytyke.com s3://backup.data.thegnarlytyke.com --delete",
    "data-test-to-live-step2": "aws s3 sync s3://test.data.thegnarlytyke.com s3://data.thegnarlytyke.com --delete",
    "data-test-to-live": "npm run data-test-to-live-step1 && npm run data-test-to-live-step2",
    "data-live-to-test": "aws s3 sync s3://data.thegnarlytyke.com s3://test.data.thegnarlytyke.com --delete",
    "data-test-to-dev": "aws s3 sync s3://test.data.thegnarlytyke.com s3://dev.data.thegnarlytyke.com --delete"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "express": "^4.17.3",
    "nodemon": "^2.0.15",
    "uuid": "^8.3.2",
    "zip-lib": "^0.7.3"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "jest": "^27.5.1",
    "watchify": "^4.0.0"
  }
}
